_link_api + '/api/bairros', {}, {}).then((data) => {\n        this.bairros = JSON.parse(data.data);\n     });\n\n     this.http.get(new Config().local_link_api + '/api/cidades', {}, {}).then((dataz) => {\n      this.cidades = JSON.parse(dataz.data);\n   });\n\n      }\n\n\n  routerlink_api(string){\n    this.zone.run(async () => {\n      await this.navCtrl.navigateForward(string);\n    });\n  }\n\n  ngOnInit() {\n  }\n\n  cadastro(){\n\n    if(this.nome_s == null){\n      this.alert_erro('ERRO', 'Falta uma informação', 'Digite o seu nome');\n      return;\n    }\n    if(this.tel_s == null){\n      this.alert_erro('ERRO', 'Falta uma informação', 'Digite o seu telefone');\n      return;\n    }\n    if(this.cep_s == null){\n      this.alert_erro('ERRO', 'Falta uma informação', 'Digite o seu cep');\n      return;\n    }\n    if(this.endereco == null){\n      this.alert_erro('ERRO', 'Falta uma informação', 'Digite o seu endereço');\n      return;\n    }\n    if(this.uf == null){\n      this.alert_erro('ERRO', 'Falta uma informação', 'Digite sua cidade');\n      return;\n    }\n    if(this.bairro_s == null){\n      this.alert_erro('ERRO', 'Falta uma informação', 'Escolha o seu bairro');\n      return;\n    }\n    if(this.numero_s == null){\n      this.alert_erro('ERRO', 'Falta uma informação', 'Digite o número');\n      return;\n    }\n    if(this.email_s == null){\n      this.alert_erro('ERRO', 'Falta uma informação', 'Digite o seu e-mail');\n      return;\n    }\n    if(this.senha_s == null){\n      this.alert_erro('ERRO', 'Falta uma informação', 'Digite sua senha');\n      return;\n    }\n  \n    /*\n    public endereco;\n    public cep_s;\n    public uf;\n    public nome_s;\n    public bairro_s;\n    public complemento_s;\n    public numero_s;\n    public email_s;\n    public senha_s;\n    */\n   \n      \n   var bodys = \n   {\n    \"nome\": this.nome_s,\n    \"cep\": this.cep_s,\n    \"endereco\": this.endereco,\n    \"uf\": this.uf.replace('/', '-'),\n    \"bairro\": this.bairro_s,\n    \"complemento\": this.complemento_s,\n    \"numero\": this.numero_s,\n    \"email\": this.email_s,\n    \"tel\": this.tel_s,\n    \"senha\": this.senha_s\n   }\n  ;\n  \n  this.http.setDataSerializer('json');\n    this.http.post(new Config().local_link_api + '/api/login', bodys, {}).then((data) => {\n    if(data.data.includes('USER_EXIST')){\n      this.alert_erro('ERRO', 'Problema ao cadastrar', 'Esse e-mail ja foi cadastrado');\n      return;\n    }else{\n      this.alert_erro('SUCESSO', 'Bem vindo', 'Conta criada com sucesso');\n      this.navCtrl.navigateRoot(\"/home\");\n    }\n  \n    \n  }).catch((err) =>{\n  \n  });\n  \n  }\n\n  async alert_erro(header, subheader, message) {\n    const alert = await this.alertCtrl.create({\n      header: header,\n      subHeader: subheader,\n      message: message,\n      buttons: ['OK']\n    });\n    await alert.present();\n \n }\n}\n"],"sourceRoot":"webpack:///","file":"cadastro-cadastro-module-es5.js"}